npx sequelize-cli model:generate --name TeacherNumber --attributes NIP:string
npx sequelize-cli model:generate --name StudentNumber --attributes NIS:string

npx sequelize-cli model:generate --name Teacher --attributes name:string,nipId:integer,password:string
npx sequelize-cli model:generate --name Student --attributes name:string,nisId:integer

npx sequelize-cli model:generate --name Course --attributes name:string,teacherId:integer
npx sequelize-cli model:generate --name Assignment --attributes name:string,teacherId:integer,studentId:integer
npx sequelize-cli model:generate --name Attendance --attributes status:string,teacherId:integer,studentId:integer

npx sequelize-cli model:generate --name Score --attributes score:integer,studentId:integer,courseId:integer